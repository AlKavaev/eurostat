citHeader("Kindly cite the eurostat R package as follows:")

citEntry(entry="misc",
         title = "eurostat R package",
         author = personList(
	   person(given ="Leo", family="Lahti", email = "louhos@googlegroups.com"),
	   person(given ="Przemyslaw", family="Biecek")
         ),
	 journal = "",
	 year = "2014",
         textVersion =
         paste("(C) Leo Lahti and Przemyslaw Biecek 2014.",
               "eurostat R package",
               sep=" ")
         )


